{
  "name": "mechanical-wombat",
  "version": "1.5.11",
  "description": "React UI component lib for edozo apps and sites",
  "author": "edozo",
  "license": "MIT",
  "repository": "edozo/mechanical-wombat",
  "homepage": "https://edozo.github.io/mechanical-wombat/",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "build-storybook",
    "storybook:clear-cache": "rm -rf node_modules/.cache/storybook",
    "storybook": "yarn storybook:clear-cache && start-storybook -p 9009",
    "storybook_win": "yarn storybook:clear-cache&start-storybook -p 9009",
    "build-storybook": "build-storybook",
    "lint": "eslint --ext .ts --ext .tsx src && yarn lint:css",
    "lintfix": "eslint --ext .ts --ext .tsx src --fix && yarn lint:css",
    "lint:css": "stylelint './src/**/*.ts'"
  },
  "dependencies": {
    "react-collapsed": "^3.0.2"
  },
  "peerDependencies": {
    "@tippyjs/react": "^4.2.5",
    "downshift": "^6.1.3",
    "polished": "^4.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.0",
    "styled-reboot": "^5.0.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@svgr/rollup": "^5.5.0",
    "@svgr/webpack": "^5.5.0",
    "@tippyjs/react": "^4.2.5",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.4",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-runtime": "^6.26.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "polished": "^4.1.3",
    "postcss": "^8.2.8",
    "prettier": "^1.19.1",
    "pretty-quick": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts-ts": "^3.1.0",
    "rollup": "^2.41.5",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-typescript2": "^0.32.1",
    "rollup-plugin-url": "^3.0.1",
    "styled-components": "^5.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^4.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "themeprovider-storybook": "^1.7.1",
    "typescript": "^4.2.4"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "git add"
    ]
  }
}
