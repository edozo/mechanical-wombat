{
  "name": "test-ui-lib",
  "version": "0.0.1",
  "description": "React UI component lib",
  "author": "martinmcneela",
  "license": "MIT",
  "repository": "martinmcneela/test-ui-lib",
  "homepage": "https://martinmcneela.github.io/test-ui-lib",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
