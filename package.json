{
  "name": "@edozo/mechanical-wombat",
  "version": "2.0.9",
  "description": "React UI component lib for edozo apps and sites",
  "author": "edozo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/edozo/mechanical-wombat.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "homepage": "https://edozo.github.io/mechanical-wombat/",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "scripts": {
    "build": "tsup && tsc -p tsconfig.types.json",
    "start": "tsup --watch",
    "prepare": "yarn run build",
    "predeploy": "build-storybook",
    "storybook:clear-cache": "rm -rf node_modules/.cache/storybook",
    "storybook": "storybook dev -p 9009",
    "storybook_win": "yarn storybook:clear-cache&storybook dev -p 9009",
    "build-storybook": "storybook build",
    "lint": "eslint --ext .ts --ext .tsx src && yarn lint:css",
    "lintfix": "eslint --ext .ts --ext .tsx src --fix && yarn lint:css",
    "lint:css": "stylelint './src/**/*.ts'",
    "test": "echo \"No tests configured\" && exit 0",
    "test:watch": "echo \"No tests configured\" && exit 0"
  },
  "dependencies": {
    "react-collapsed": "2.0.0"
  },
  "peerDependencies": {
    "@tippyjs/react": "^4.2.5",
    "downshift": "^6.1.3",
    "polished": "^4.1.3",
    "react": ">=18 <20",
    "react-dom": ">=18 <20",
    "styled-components": "^6.1.19",
    "styled-reboot": "^5.0.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.2",
    "@storybook/react-webpack5": "^9.1.2",
    "@svgr/rollup": "^5.5.0",
    "@svgr/webpack": "^5.5.0",
    "@tippyjs/react": "^4.2.5",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "downshift": "^6.1.3",
    "esbuild-plugin-svgr": "^2.1.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "eslint-plugin-storybook": "9.1.2",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "polished": "^4.1.3",
    "postcss": "^8.5.6",
    "prettier": "^1.19.1",
    "pretty-quick": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^9.1.2",
    "styled-components": "^6.1.19",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^4.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.4.5",
    "@storybook/react-vite": "^9.1.2",
    "vite": "^5.4.0",
    "vite-plugin-svgr": "^4.2.0",
    "@mdx-js/react": "^3.0.1"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "git add"
    ]
  },
  "packageManager": "yarn@1.22.22"
}
